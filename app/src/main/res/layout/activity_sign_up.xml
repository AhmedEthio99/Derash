<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/registbackground"
    tools:context=".SignUp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:fontFamily="sans-serif-smallcaps"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.179"
        tools:ignore="HardcodedText" />


    <EditText
        android:id="@+id/editTextFirstName"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:background="@drawable/backgroungdriver"
        android:drawableLeft="@drawable/ic_baseline_person_24"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.245"
        tools:ignore="MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/editTextMidName"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_person_24"
        android:hint="Middle Name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.342"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/editTextLastName"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_person_24"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.442"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/editTextEmail1"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_email_24"
        android:hint="Email"
        android:minHeight="48dp"
        android:paddingStart="10dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.628"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/editTextPhonenum"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_phone_android_24"
        android:hint="Phone Number"
        android:minHeight="48dp"
        android:paddingStart="10dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/password1"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:autofillHints=""
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_lock_24"
        android:hint="Password"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:paddingRight="17dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.714" />

    <EditText
        android:id="@+id/password2"
        android:layout_width="325dp"
        android:layout_height="44dp"
        android:autofillHints=""
        android:background="@drawable/backgroungdriver"
        android:drawableStart="@drawable/ic_baseline_lock_24"
        android:hint="Confirm Password"
        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:paddingRight="17dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.806" />

    <Button
        android:id="@+id/button3"
        android:layout_width="226dp"
        android:layout_height="48dp"
        android:text="Sign Up"
        app:backgroundTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.906" />

</androidx.constraintlayout.widget.ConstraintLayout>